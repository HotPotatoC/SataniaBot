{
	"name": "satania-bot",
	"version": "1.0.0",
	"description": "The most devilish bot for Discord!",
	"main": "main.js",
	"author": "Pizzacus",
	"license": "MIT",
	"homepage": "https://satania.moe/",
	"repository": "https://github.com/Pizzacus/SataniaBot.git",
	"private": true,
	"dependencies": {
		"cheerio": "^1.0.0-rc.3",
		"discord-akairo": "^7.5.6",
		"discord.js": "^11.5.1",
		"file-type": "^12.0.0",
		"js-yaml": "^3.13.1",
		"make-fetch-happen": "^4.0.1",
		"money": "^0.2.0",
		"regenerate": "^1.4.0",
		"seedrandom": "^3.0.1",
		"sharp": "^0.23.2",
		"simple-markdown": "0.4.4",
		"tough-cookie": "^3.0.1"
	},
	"optionalDependencies": {
		"erlpack": "discordapp/erlpack",
		"@discordjs/uws": "^10.149.0"
	},
	"devDependencies": {
		"eslint": "^6.0.1",
		"eslint-config-xo": "^0.26.0",
		"regexgen": "^1.3.0",
		"xo": "^0.24.0"
	},
	"scripts": {
		"lint": "xo"
	},
	"xo": {
		"rules": {
			"no-await-in-loop": 0,
			"unicorn/no-process-exit": 0,
			"prefer-destructuring": 0,
			"valid-jsdoc": [
				"error",
				{
					"matchDescription": "^[^a-z]",
					"preferType": {
						"String": "string",
						"Number": "number",
						"Boolean": "boolean",
						"Symbol": "symbol",
						"object": "Object",
						"function": "Function",
						"array": "Array",
						"date": "Date",
						"error": "Error"
					},
					"prefer": {
						"return": "returns",
						"arg": "param",
						"argument": "param",
						"prop": "property",
						"yield": "yields"
					}
				}
			],
			"eqeqeq": [
				"error",
				"always",
				{
					"null": "ignore"
				}
			],
			"no-eq-null": [
				"off"
			]
		},
		"globals": [
			"requireUtil"
		],
		"ignores": [
			"src/utils/emoji-regex.js",
			"src/utils/east-asian-width-regex.js"
		]
	}
}
